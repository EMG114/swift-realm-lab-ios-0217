# platform :ios, '9.0'

target 'TvGuideRealmLab' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  def commonPods
    pod 'Realm'
    pod 'RealmSwift'
  end

  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'Kingfisher'
  commonPods

  # Pods for TvGuideRealmLab

  target 'TvGuideRealmLabTests' do
    inherit! :search_paths
    commonPods
    # Pods for testing
  end

end
